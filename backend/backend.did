type Activity = 
 record {
   activityType: text;
   currency: text;
   date: int;
   fee: float64;
   quantity: float64;
   symbol: text;
   unitPrice: float64;
 };
service : {
  addActivity: (text, text, float64, text, float64, text, float64) -> ();
  getActivities: () -> (vec Activity) query;
  getAllocations: () ->
   (record {
      cash: float64;
      crypto: float64;
      equity: float64;
      fixedIncome: float64;
    }) query;
  importActivities: (vec Activity) -> ();
}
