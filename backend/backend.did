type Holding = 
 record {
   assetType: text;
   currentPrice: float64;
   name: text;
   purchasePrice: float64;
   quantity: float64;
   sector: text;
   symbol: text;
 };
service : {
  addHolding: (text, text, float64, float64, float64, text, text) -> ();
  getAllocations: () ->
   (record {
      cash: float64;
      crypto: float64;
      equity: float64;
      fixedIncome: float64;
    }) query;
  getHoldings: () -> (vec Holding) query;
}
